{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Robot": {
    "Host": "192.168.5.1", // ROBOT IP'sini buraya yaz
    "Port": 6601, // ROBOT portunu buraya yaz
    "Encoding": "ASCII", // "ASCII" | "UTF8" | "Unicode"
    "AppendNewLine": true, // true ise her komutun sonuna \n ekler
    "ConnectTimeoutMs": 8000 // bağlanma zaman aşımı
  },

  "AllowedHosts": "*"
}